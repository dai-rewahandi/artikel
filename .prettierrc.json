{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "bracketSameLine": true,
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "printWidth": 120,
  "trailingComma": "all"
}
